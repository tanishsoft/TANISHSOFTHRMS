            success: function (data) {
                $("#myModalEdit").modal('show');
                var obj = data;
                $("[name=eRadioTitle]").val([obj.DR_Title]);
                $("#eID").val(obj.DR_Id);
                $("#eFirstName").val(obj.DR_FirstName);
                $("#eLastName").val(obj.DR_LastName);
                $("#eDateofBirth").val(obj.DR_DateOfBirth);
                $("[name=eRadioGender]").val([obj.DR_Gender]);
                $("#eBloodGroup").val(obj.DR_BloodGroup);
                $("#eNationality").val(obj.DR_Nationality);
                $("#eMobile").val(obj.DR_mobileNo);
                $("#eEmail").val(obj.DR_Email);
                $("#eQualification").val(obj.DR_Qualification);
                $("#eRegistrationNo").val(obj.DR_RegistrationNumber);
                $("#eRegistrationState").val(obj.DR_RegistrationState);
                $("#eExperience").val(obj.DR_Experience);
                $("#eSpecialication").val(obj.DR_specialization);
                $("#eFlatNo").val(obj.DR_residence_FlotNo);
                $("#eRoadNo").val(obj.DR_residence_RoadName);
                $("#eArea").val(obj.DR_residence_locality);
                $("#eCity").val(obj.DR_residence_city);
