}

<h2>DoctorRegistration</h2>

<script>
    $(document).ready(function () {
        LoadData();
        $(".date")
            .datepicker({
                changeMonth: true,
                changeYear: true,
                dateFormat: "dd-mm-yy",
                Edit: true

            })
    });
    function isNumber(evt) {

        var iKeyCode = (evt.which) ? evt.which : evt.keyCode
        if (iKeyCode != 46 && iKeyCode > 31 && (iKeyCode < 48 || iKeyCode > 57)) {
            return false;
        } else {
            return true;
        }
    }

    function LoadData() {
        var oTable = $('#tblDoctorRegistration').dataTable({
            "bServerSide": true,
            "sAjaxSource": "/Doctor/AjaxGetDoctorRegistration",
            "bProcessing": true,
            "bDestroy": true,
            "aLengthMenu": [[10, 20, 30, 50, 100, 150, 200, -1], [10, 20, 30, 50, 100, 150, 200, "All"]],
            "aoColumns": [
                { "sName": "DR_Id" },
                { "sName": "DR_FirstName" },
                { "sName": "DR_mobileNo" },
                { "sName": "DR_Email" },
                { "sName": "DR_DateOfBirth" },
                { "sName": "DR_Gender" },
                { "sName": "DR_Qualification" },
                { "sName": "DR_specialization" },
                {
                    "sName": "DR_Id",
                    "bSortable": false,
                    "render": function (data) {
                        return '<div class="btn-group">' +
                            '<div class="btn btn-default dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-th-list"></span></div>' +
                            '<ul class="dropdown-menu dropdown-menu-right">' +
                            '<li><a href="javascript:void(0);" id="' + data + '" onclick="Edit(this.id);">Edit</a></li>' +
                            '<li><a href="javascript:void(0);" id="' + data + '" onclick="Delete(this.id);">Delete</a></li>' +
                            '</ul></div>';
                    }
                }
            ]
        });
        $("#tblDoctorRegistration_length").addClass("col-md-6");
        $("#tblDoctorRegistration_length").addClass("col-md-6");
    }
    function AjaxPost() {
        debugger;
        validateNew();
        if (validate) {
            $(".error").hide();
            $(".css-error").removeClass('css-error');
            // Checking whether FormData is available in browser
            if (window.FormData !== undefined) {
                // Create FormData object
                var fileData = new FormData();

                // Adding one more key to FormData object
                fileData.append('DR_Title', $('input[name=RadioTitle]:checked').val());
                fileData.append('DR_FirstName', $("#FirstName").val());
                fileData.append('DR_LastName', $("#LastName").val());
                fileData.append('DR_DateOfBirth', $("#DateofBirth").val());
                fileData.append('DR_Gender', $('input[name=RadioGender]:checked').val());
                fileData.append('DR_BloodGroup', $("#BloodGroup").val());
                fileData.append('DR_Nationality', $("#Nationality").val());
                fileData.append('DR_mobileNo', $("#Mobile").val());
                fileData.append('DR_Email', $("#Email").val());
                fileData.append('DR_Qualification', $("#Qualification").val());
                fileData.append('DR_RegistrationNumber', $("#RegistrationNo").val());
                fileData.append('DR_RegistrationState', $("#RegistrationState").val());
                fileData.append('DR_Experience', $("#Experience").val());
                fileData.append('DR_specialization', $("#Specialication").val());
                fileData.append('DR_residence_FlotNo', $("#FlatNo").val());
                fileData.append('DR_residence_RoadName', $("#RoadNo").val());
                fileData.append('DR_residence_locality', $("#Area").val());
                fileData.append('DR_residence_city', $("#City").val());
                fileData.append('DR_residence_State', $("#state").val());
                fileData.append('DR_residence_country', $("#Country").val());
                fileData.append('DR_residence_PhoneNo', $("#Telephone").val());
                fileData.append('DR_residence_office', $("#Office").val());
                fileData.append('DR_residence_fax', $("#fax").val());
                fileData.append('DR_residence_pincode', $("#pincode").val());
                $.ajax({
                    type: "POST",
                    url: "/Doctor/SaveDOctorRegistration",
                    contentType: false, // Not to set any content header
                    processData: false, // Not to process data
                    data: fileData,
                    success: function (result) {
                        alert(result);
                        clearAll();

                    },
                    error: function (err) {
                        alert(err.statusText);
                    }
                });
            } else {
                alert("FormData is not supported.");
            }
        }
        else {
            return false;
        }

    }
    function Edit(DorId) {
        debugger;
        var Url = "/Doctor/GetDoctorById?DRId=" + DorId;
