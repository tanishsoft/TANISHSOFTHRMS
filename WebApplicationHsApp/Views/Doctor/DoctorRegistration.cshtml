                validate = false;
            }
            if ($("#eBloodGroup").val() == "") {
                $("#eBloodGroup").addClass('css-error');
                validate = false;
            }
            if ($("#eNationality").val() == "") {
                $("#eNationality").addClass('css-error');
                validate = false;
            }
            if ($("#eMobile").val() == "") {
                $("#eMobile").addClass('css-error');
                validate = false;
            }
            var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
            var emailaddressVal = $("#eEmail").val();
            if ($("#eEmail").val() == "") {
                $("#eEmail").addClass('css-error');
                validate = false;
            }
            else if (!emailReg.test(emailaddressVal)) {
                $("#eEmail").addClass('css-error');
                $("#eEmail").after('<span class="error" style="color:Red;">Enter a valid email address.</span>');
                validate = false;
            }
            if ($("#eQualification").val() == "") {
                $("#eQualification").addClass('css-error');
                validate = false;
            }
            if ($("#eRegistrationNo").val() == "") {
                $("#eRegistrationNo").addClass('css-error');
                validate = false;
            }
            if ($("#eSpecialication").val() == "") {
                $("#eSpecialication").addClass('css-error');
                validate = false;
            }
        }
</script>
