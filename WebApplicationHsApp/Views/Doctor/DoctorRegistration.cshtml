    var Delete = function (Id) {
        $("#Cid").val(Id);
        $("#DeleteConfirmation").modal("show");
    }
    var ConfirmDelete = function () {
        var CId = $("#Cid").val();
        $.ajax({
            type: "POST",
            url: "/Doctor/DeleteDR?DRSId=" + CId,
            success: function (result) {
                $("#DeleteConfirmation").modal("hide");
                LoadData();
            }
        })
    }
    function validateNew() {
        debugger;
        validate = true;
        $(".error").hide();
        $(".css-error").removeClass('css-error');
        if ($("#FirstName").val() == "") {
            $("#FirstName").addClass('css-error');
            validate = false;
        }
        if ($("#LastName").val() == "") {
            $("#LastName").addClass('css-error');
            validate = false;
        }
        if ($("#DateofBirth").val() == "") {
            $("#DateofBirth").addClass('css-error');
            validate = false;
        }
        if ($('input[type=radio][name=RadioGender]:checked').length == 0) {
            $("#RadioGender").addClass('css-error');
            validate = false;
        }
        if ($("#BloodGroup").val() == "") {
            $("#BloodGroup").addClass('css-error');
            validate = false;
        }
        if ($("#Nationality").val() == "") {
            $("#Nationality").addClass('css-error');
            validate = false;
        }
        if ($("#Mobile").val() == "") {
            $("#Mobile").addClass('css-error');
            validate = false;
        }
        var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
        var emailaddressVal = $("#Email").val();
        if ($("#Email").val() == "") {
            $("#Email").addClass('css-error');
            validate = false;
        }
        else if (!emailReg.test(emailaddressVal)) {
            $("#Email").addClass('css-error');
            $("#Email").after('<span class="error" style="color:Red;">Enter a valid email address.</span>');
            validate = false;
        }
        if ($("#Qualification").val() == "") {
            $("#Qualification").addClass('css-error');
            validate = false;
        }
        if ($("#RegistrationNo").val() == "") {
            $("#RegistrationNo").addClass('css-error');
            validate = false;
        }
        if ($("#Specialication").val() == "") {
            $("#Specialication").addClass('css-error');
            validate = false;
        }
        }
