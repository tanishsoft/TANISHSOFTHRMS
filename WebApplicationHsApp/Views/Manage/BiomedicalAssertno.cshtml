
@{
    ViewBag.Title = "BiomedicalAssertno";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
    function Saveassertname() {
        var validate = true;      
        if (validate) {          
            var data = {
                AssertID: $("#txtassertid").val(),
                Assertname: $("#txtassertname").val()
                
            };
            $.ajax({
                type: 'POST',
                url: "/Manage/SaveAssert",
                data: "data=" + JSON.stringify(data),
                success: function (result) {
                    alert(result);                  
                    window.location.reload();
                }
            });
        }
    }
</script>

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">Manage Assert Id & Names </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="col-xs-1">

                            <label for="name">Assert Id </label>
                        </div>
                        <div class="col-xs-3">
                            <input type="text" name="name" id="txtassertid" class="form-control" />                           

                        </div>
                        <div class="col-xs-1">

                            <label for="name">Assert Name </label>
                        </div>
                        <div class="col-xs-3">
                            <input type="text" name="name" id="txtassertname" class="form-control" />   

                        </div>
                        <div class="col-xs-2">
                            <input type="button" name="name" value="Submit" class="btn btn-primary" onclick="Saveassertname()" />
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



