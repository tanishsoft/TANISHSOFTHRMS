            },

            legend: {
                enabled: false
            },

            plotOptions: {
                series: {
                    borderWidth: 0,
                    dataLabels: {
                        enabled: true
                    }
                }
            },

            series: [{
                name: 'PM',
                colorByPoint: true,
                data: locations
            }],

            drilldown: {
                series: []
            }
        });
    }
</script>
