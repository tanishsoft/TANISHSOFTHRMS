                alert("error")
            }
        });
    }
    function LoadheadcountofPMMonthWise() {
        $.ajax({
            type: "POST",
            url: "/Biomedical/GetheadcountofPMByMonth",
            //data: "id=",
            success: function (data) {
                debugger;
                var locdata = new Array();
                for (var i = 0; i < data.length; i++) {
                    locdata.push({
                        name: data[i].key,
                        y: data[i].cnt,
                        drilldown: true
                    });
                }
                BindCountMonthwisePM(locdata);
            },
            error: function () {
                alert("error")
            }
        });
    }

    function BindheadcountofEmployeeforDailyRounds(locations) {

        $('#container4').highcharts({
            chart: {
                renderTo: 'lineChart',
                //type: 'line',
                //events: {

                //},
            },
            title: {
                text: 'Daily Rounds Head Count by Employee'
            },
            xAxis: {
                type: 'category',
            },

            legend: {
                enabled: false
            },

            plotOptions: {
                series: {
                    visible: true,
                    dataLabels: {
                        enabled: true,
                        step: 'left'
                    }
                }
            },

            series: [{
                name: 'Dalily Rounds Head Count',
                colorByPoint: true,
                lineWidth: 2,
                connectNulls: true,
                data: locations
            }],
        });



    }
    function BindheadcountofEmployeeforBreakDown(locations) {

        $('#container5').highcharts({
            chart: {
                renderTo: 'lineChart',
                //type: 'line',
                //events: {

                //},
            },
            title: {
                text: 'BreakDown Head Count by Employee'
            },
            xAxis: {
                type: 'category',
            },

            legend: {
                enabled: false
            },

            plotOptions: {
                series: {
                    visible: true,
                    dataLabels: {
                        enabled: true,
                        step: 'left'
                    }
                }
            },

            series: [{
                name: 'BreakDown Head Count',
                colorByPoint: true,
                lineWidth: 2,
                connectNulls: true,
                data: locations
            }],
        });



    }
    function BindheadcountofBMbyEquipment(locations) {

        $('#container6').highcharts({
            chart: {
                renderTo: 'lineChart',
                //type: 'line',
                //events: {

                //},
            },
            title: {
                text: 'BreakDown Head Count by Equipments'
            },
            xAxis: {
                type: 'category',
            },

            legend: {
                enabled: false
            },

            plotOptions: {
                series: {
                    visible: true,
                    dataLabels: {
                        enabled: true,
                        step: 'left'
                    }
                }
            },

            series: [{
                name: 'Break Down Head Count',
