                for (var i = 0; i < data.length; i++) {
                    locdata.push({
                        name: data[i].key,
                        y: data[i].cnt,
                        drilldown: true
                    });
                }
                BindheadcountofEmployeeforDailyRounds(locdata);
            },
            error: function () {
                alert("error")
            }
        });
    }


    function LoadheadcountofBreakDown() {
        $.ajax({
            type: "POST",
