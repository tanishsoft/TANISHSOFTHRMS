<script>
    $(document).ready(function () {
        $(".BreakDown").hide();
        $(".PreventiveMaintenance").hide();
    });
    function typechange() {
        var typevalue = $("#ddltype").val();
        if (typevalue == "DailyRounds") {
            $(".BreakDown").hide();
            $(".PreventiveMaintenance").hide();
        } else if (typevalue == "BreakDown") {
            $(".PreventiveMaintenance").hide();
            $(".BreakDown").show();            
        }
        else if (typevalue == "PreventiveMaintenance") {
            $(".BreakDown").hide();
            $(".PreventiveMaintenance").show();
        }
    }
</script>
