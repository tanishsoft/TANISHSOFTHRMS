<div id="myModalBreakDown" class="modal fade" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog" style="width:750px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" onclick="clearAll();">×</button>
                <h4 class="modal-title" id="header">New Task</h4>
            </div>
            <div class="modal-body">
                <div class="row" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Type<span class="text-danger">*</span></label>
                        <select class="form-control" id="ddltype" onchange="typechange();">
                            <option value="DailyRounds">Daily Rounds</option>
                            <option value="BreakDown">Break Down</option>
                            <option value="PreventiveMaintenance">Preventive Maintenance</option>
                        </select>
                    </div>
                    <div class="col-md-6">
                        <label>Category Of Service<span class="text-danger">*</span></label>
                        <select class="form-control" id="ddlCategoryId"></select>
                    </div>
                </div>
                <div class="row" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Assign Location<span class="text-danger">*</span> </label>
                        <select class="form-control" id="ddlAssignDepartmentId"></select>
                    </div>
                    <div class="col-md-6">
                        <label>Assign Department<span class="text-danger">*</span> </label>
                        <select class="form-control" id="ddlAssignDepartmentId"></select>
                    </div>
                </div>
                <div class="row" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Sub Department<span class="text-danger">*</span> </label>
                        <select class="form-control" id="ddlAssignDepartmentId"></select>
                    </div>
                    <div class="col-md-6">
                        <label>Document</label>
                        <input type="file" id="fleDocument" class="form-control" />
                    </div>
                </div>
                <div class="row BreakDown PreventiveMaintenance" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Asset Number<span class="text-danger">*</span> </label>
                        <input type="text" class="form-control" id="txtAssetNumber" />
                    </div>
                    <div class="col-md-6">
                        <label>Equipment<span class="text-danger">*</span></label>
                        <input type="text" class="form-control" id="txtEquipmentName" />
                    </div>
                </div>
                <div class="row BreakDown PreventiveMaintenance" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Spare Replaced<span class="text-danger">*</span> </label>
                        <input type="text" class="form-control" id="txtSpareReplaced" />
                    </div>
                    <div class="col-md-6">
                        <label>Job Done</label>
                        <select class="form-control" id="ddlVendorId"></select>
                    </div>
                </div>
                <div class="row" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Start Time<span class="text-danger">*</span> </label>
                        <input type="text" id="txtStartDate" class="form-control" />
                    </div>
                    <div class="col-md-6">
                        <label>End Time<span class="text-danger">*</span></label>
                        <input type="text" id="txtEndDate" class="form-control" />
                    </div>
                </div>
                <div class="row BreakDown" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Problem Observed<span class="text-danger">*</span> </label>
                        <textarea class="form-control textarea" id="txtProblemObserved"></textarea>
                    </div>
                    <div class="col-md-6">
                        <label>Problem Reported<span class="text-danger">*</span> </label>
                        <textarea class="form-control textarea" id="txtProblemReport"></textarea>
                    </div>
                </div>
                <div class="row" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6">
                        <label>Call status<span class="text-danger">*</span> </label>
                        <select class="form-control" id="ddlCallStatusId"></select>
                    </div>
                    <div class="col-md-6">
                        <label>Work Done Description<span class="text-danger">*</span> </label>
                        <textarea class="form-control textarea" id="txtWorkDoneDescription"></textarea>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <input type="button" id="btnasigntask" value="Save" onclick="OnclickSavechanges();" class="btn btn-primary">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="clearAll();">Close</button>
            </div>
        </div>
    </div>
</div>
