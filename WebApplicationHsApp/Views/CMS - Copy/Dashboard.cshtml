    <div class="row">
        <div class="col-lg-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <i class="fa fa-bar-chart-o fa-fw"></i> My Orders
                </div>
                <div class="panel-body">
                    <div id="DivMyOrders">
                        <table class="table table-striped" style="width:100%;">
                            <thead>
                                <tr style="background-color:crimson;color:white;">
                                    <td>Id</td>
                                    <td>Item</td>
                                    <td>Date</td>
                                    <td>Qty</td>
                                    <td>Comments</td>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.myOrders != null && Model.myOrders.Count > 0)
                                {
                                    for (int i = 0; i < Model.myOrders.Count; i++)
                                    {
                                        <tr>
                                            <td>@Model.myOrders[i].Id</td>
                                            <td>@Model.myOrders[i].Item</td>
                                            <td>@Model.myOrders[i].Date.ToString("dd/MM/yyyy")</td>
                                            <td>@Model.myOrders[i].Qty</td>
                                            <td>@Model.myOrders[i].Comments</td>
                                        </tr>
                                    }

                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <i class="fa fa-bar-chart-o fa-fw"></i> My Payments - Balance Rs. @Model.TotalAmount
                </div>
                <div class="panel-body">
                    <table class="table table-striped" style="width:100%;">
                        <thead>
                            <tr style="background-color:chocolate;color:white;">
                                <td>Id</td>
                                <td>Type</td>
                                <td>Date</td>
                                <td>Amount</td>
                                <td>Comments</td>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.myPayments != null && Model.myPayments.Count > 0)
                            {
                                for (int i = 0; i < Model.myPayments.Count; i++)
                                {
                                    <tr>
                                        <td>@Model.myPayments[i].Id</td>
                                        <td>@Model.myPayments[i].Type</td>
                                        <td>@Model.myPayments[i].Date.ToString("dd/MM/yyyy")</td>
                                        <td>@Model.myPayments[i].Amount</td>
                                        <td>&nbsp;</td>
                                    </tr>
                                }

                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
<div id="myModalBrandedMerchandise" class="modal fade" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" onclick="clearAll();">×</button>
                <h4 class="modal-title"> Order Place</h4>
            </div>
            <div class="modal-body">

                <div class="row" style="padding:5px;">
                    <div class="col-md-4"><label for="name">Employee Id<span style="color:red;">*</span></label></div>
                    <div class="col-md-8">
                        <input type="text" class="form-control" value="@User.Identity.Name" id="EmployeeId" />
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4"><label for="name">Employee Name<span style="color:red;">*</span></label></div>
                    <div class="col-md-8">
                        <input type="text" class="form-control" id="EmployeeName" />
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4"><label for="name">Item<span style="color:red;">*</span></label></div>
                    <div class="col-md-8">
                        <input type="text" class="form-control" id="BrandProduct" />
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4"><label for="name">Required quantity<span style="color:red;">*</span></label></div>
                    <div class="col-md-8">
                        <input type="number" value="1" class="form-control" id="requiredquantity" />
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4"><label for="name">Comments<span style="color:red;">*</span></label></div>
                    <div class="col-md-8">
                        <textarea class="form-control" id="Comments"> OK  </textarea>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <input type="button" id="btnasigntask" value="Submit Order" onclick="SendEmail();" class="btn btn-primary">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="clearAll();">Close</button>
            </div>
        </div>

    </div>
</div>