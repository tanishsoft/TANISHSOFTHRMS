    function CancelLeave(id) {
        var r = confirm("Are you sure!");
        if (r == true) {
            $.ajax({
                type: "POST",
                url: "/Hr/DeleteLeave",
                data: "id=" + id,
                success: function (data) {
                    ApprovedDataTable();
                },
                error: function () {
                    alert("error")
                }
            });
        }

    }
    function EditLeave(id) {
        $.ajax({
            type: "POST",
            url: "/HrAdmin/GetEditLeaveDetailsByID",
            data: "id=" + id,
            success: function (data) {
                if (data != null) {
                    //$("#blockLeave").html(data);
                    BindEditLeaveDataToModel_PageLevel(data);
                }
            },
            error: function () {
                alert("error")
            }
        });
    }
    var EditLeaveDataID = 0;
    function BindEditLeaveDataToModel_PageLevel(EditLeaveData) {
        EditLeaveDataID = parseFloat(EditLeaveData.LeaveId);
        var IsFullDayLeave = (EditLeaveData.IsFullday == "True") ? true : false;
        var IsCompOFfLeave = (EditLeaveData.IsCompOff == "True") ? true : false;
        $("#UddlLeaveType1").val(EditLeaveData.LeaveTypeId);
        $("#ULeaveFromDate").val(functionDate(EditLeaveData.LeaveFromDate));
        $("#UDateofAvailableCompoff").val(EditLeaveData.DateofAvailableCompoff_CustomFormat_1);
        $("#UReasonForLeave").val(EditLeaveData.ReasonForLeave);
        $("#UsAddressOnLeave").val(EditLeaveData.AddressOnLeave);
        $("#Ucbfullday").prop("checked", IsFullDayLeave);
        $("#UcbIsCompOff").prop("checked", IsCompOFfLeave);
        cbcheckedchangeForEdit(IsCompOFfLeave);
        $("#ULeaveTodate").val(functionDate(EditLeaveData.LeaveTodate));
        $("#myModalEditLeave").modal("show");
        DatePickerMultiUseFun_GlobalValidationJS();
    }
    function UpdateLeave() {
        if (ValidateLeaveDataBeforeUpdate()) {
            var task = {
                DateofAvailableCompoff: $("#DateofAvailableCompoff1").val() != undefined ? $("#DateofAvailableCompoff1").val() : "",
                LeaveFromDate: $("#LeaveFromDate1").val() != undefined ? $("#LeaveFromDate1").val() : "",
                LeaveTodate: $("#LeaveTodate1").val() != undefined ? $("#LeaveTodate1").val() : "",
                LeaveId: $("#LeaveId1").val() != undefined ? $("#LeaveId1").val() : "",
            };
            var fullday = $('input:checkbox[name=Ucbfullday]').is(':checked');
            var CompOff = $('input:checkbox[name=UcbIsCompOff]').is(':checked');

            var task = {
                LeaveId: EditLeaveDataID,
                LeaveTypeId: $("#UddlLeaveType1").val(),
                LeaveTypeName: $("#UddlLeaveType1 option:selected").text(),
                IsFullday: fullday,
                IsCompOff: CompOff,
                DateofAvailableCompoff: ConvertDatefromoneddmmyyytommddyyy($("#ULeaveFromDate").val()),
                LeaveFromDate: ConvertDatefromoneddmmyyytommddyyy($("#ULeaveFromDate").val()),
                LeaveTodate: ConvertDatefromoneddmmyyytommddyyy($("#ULeaveTodate").val()),
                ReasonForLeave: $("#UReasonForLeave").val(),
                AddressOnLeave: $("#UsAddressOnLeave").val(),
            };
            $.ajax({
                type: "POST",
                url: "/HrAdmin/HrUpdateLeaveManagement",
                data: task,
                success: function (data) {
                    alert(data);
                    ApprovedDataTable();
                    $("#myModalEditLeave").modal("hide");
                },
                error: function () {
                    alert("error")
                }
            });
        }
    }
    function dropdowncheckedchange(id) {
        if ($("#" + id).val() == "6") {
            $("#trfromdate").addClass("trCompoff");
        }
        else {
            $("#trfromdate").removeClass("trCompoff");
        }
    }
    function ShowHideElementByElementID(ElementID, Status) {
        $("#" + ElementID).hide();
        if (Status) {
            $("#" + ElementID).show();
        }
    }
    function ValidateLeaveDataBeforeUpdate() {
        var fullday = $('input:checkbox[name=Ucbfullday]').is(':checked');
        var CompOff = $('input:checkbox[name=UcbIsCompOff]').is(':checked');
        $("#ULeaveFromDate").removeClass("EditDataRequired");
        $("#UDateofAvailableCompoff").removeClass("EditDataRequired");
        if (CompOff) {
            $("#UDateofAvailableCompoff").addClass("EditDataRequired");
        } else {
            $("#ULeaveFromDate").addClass("EditDataRequired");
        }
        var Valid = ValidateRequiredFieldsByClassName_GlobalJS("EditDataRequired");
        return Valid;
    }
    function ExporttoExcel() {
        window.location = "/HrAdmin/ExportToExcelEmployeesLeaveApplications?location=" + $("#ddlLocation").val() + "&department=" + $("#ddlDepartment").val() + "&FromDate=" + $("#fromdate").val() + "&ToDate=" + $("#todate").val()+"&Employee="+$("#ddlEmployees").val()+"&LeaveType="+$("#ddlLeaveType").val();
    }
</script>
