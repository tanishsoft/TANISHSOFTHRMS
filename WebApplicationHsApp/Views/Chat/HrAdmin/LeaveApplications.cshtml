    function SaveLeave() {
        var LeaveType = $("#ddlLeaveType").val();
        var LeaveFromDate = $("#LeaveFromDate").val();
        var LeaveToDate = $("#LeaveTodate").val();

        var CompOff = false;
        if (LeaveType == 6) {

            $("#LeaveFromDate").val(LeaveToDate);
            $("#DateofAvailableCompoff").val(LeaveToDate);
            CompOff = true;
        }
        validateLeave();

        if (validate) {

            var fullday = $('input:checkbox[name=cbfullday]').is(':checked');
            var CompOff = $('input:checkbox[name=cbIsCompOff]').is(':checked');
            var skillsSelect = document.getElementById("ddlLocation1");
            var selectedText = skillsSelect.options[skillsSelect.selectedIndex].text;

            var deptSelect = document.getElementById("ddlDepartment1");
            var deptText = deptSelect.options[deptSelect.selectedIndex].text;

            var custSelect = document.getElementById("ddlEmployees1");
            var custText = custSelect.options[custSelect.selectedIndex].text;

            var task = {
                LocationId: $("#ddlLocation1").val(),
                DepartmentId: $("#ddlDepartment1").val(),
                LocationName: selectedText,
                DepartmentName: deptText,
                UserId: $("#ddlEmployees1").val(),
                UserName: custText,
                LeaveTypeId: $("#ddlLeaveType1").val(),
                LeaveTypeName: $("#ddlLeaveType1 option:selected").text(),
                IsFullday: fullday,
                IsCompOff: CompOff,
                DateofAvailableCompoff: $("#LeaveTodate").val(),
                LeaveFromDate: $("#LeaveFromDate").val(),
                LeaveTodate: $("#LeaveTodate").val(),
                ReasonForLeave: $("#ReasonForLeave").val(),
                AddressOnLeave: $("#AddressOnLeave").val()


            };
            $.ajax({
                type: "POST",
                url: "/Hr/HrSaveLeaveManagement",
                data: task,
                success: function (data) {
                    alert(data);
                    clearAll();
                    $("#myModalLeave").modal("hide");
                },
                error: function () {
                    alert("error")
                }
            });
        }
        else {
            alert("Fields Required");
        }
    }

    function validateLeave() {
        validate = true;
        $(".css-error").removeClass('css-error');

        if ($("#ddlLeaveType1").val() == "") {
            $("#ddlLeaveType1").addClass('css-error');
            validate = false;
        }

        if ($("#ddlLocation1").val() == " ") {
            $("#ddlLocation1").addClass('css-error');
            validate = false;
        }
        if ($("#ddlDepartment1").val() == " ") {
            $("#ddlDepartment1").addClass('css-error');
            validate = false;
        }
        if ($("#ddlEmployees1").val() == " ") {
            $("#ddlEmployees1").addClass('css-error');
            validate = false;
        }
        if ($("#LeaveFromDate").val() == "") {
            $("#LeaveFromDate").addClass('css-error');
            validate = false;
        }
        if ($("#LeaveTodate").val() == "") {
            $("#LeaveTodate").addClass('css-error');
            validate = false;
        }
        if ($("#ReasonForLeave").val() == "") {
            $("#ReasonForLeave").addClass('css-error');
            validate = false;
        }
        if ($("#AddressOnLeave").val() == "") {
            $("#AddressOnLeave").addClass('css-error');
            validate = false;
        }

    }

    function cbcheckedchange(value) {

        if (value) {
            $("#trCompoff").removeClass("trCompoff");
            $("#trfromdate").addClass("trCompoff");
        }
        else {
            $("#trfromdate").removeClass("trCompoff");
            $("#trCompoff").addClass("trCompoff");
        }
    }


    function cbcheckedchangeForEdit(value) {
        ShowHideElementByElementID("Utrfromdate", false);
        ShowHideElementByElementID("UtrCompoff", false);
        if (value) {
            ShowHideElementByElementID("UtrCompoff", true);
        }
        else {
            ShowHideElementByElementID("Utrfromdate", true);
        }
    }

