<script>
    var eId = @ViewBag.CanteenId;
    var captureSaleId =@ViewBag.id;
    var ModeofPayment = "";
    var tableRowCount = 1;
    $(document).ready(function () {
        debugger;


        $("#NoCapture").hide();
        $(".capture").hide();
        $('.iconclass1').change(function () {
            debugger;
            $('div.iconclass').removeClass('iconclass');
            $('div.rdn').addClass('iconclass1');

            $(this).closest('.rdn').removeClass('iconclass1');
            $(this).closest('.rdn').addClass('iconclass');
        });
        $('.add').click(function () {
            if ($(this).prev().val() < 30) {
                $(this).prev().val(+$(this).prev().val() + 1);
            }
        });
        $('.sub').click(function () {
            if ($(this).next().val() > 1) {
                if ($(this).next().val() > 1) $(this).next().val(+$(this).next().val() - 1);
            }
        });
        $("#InPatientd").on("propertychange change keyup paste input", function () {
            var id = $('#InPatientd').val();
            if (id != "") {
                $.get("/CMS/GetCMS_PatitentNamebyMRno?id=" + id, function (res) {
                    if (res != null) {
                        $("#InPatientdRoomNo").val(res.RoomNo);
                        $("#Name").val(res.Name);
                    }
                    else {
                        $("#InPatientdRoomNo").val("");
                        $("#Name").val("");
                    }
                });
            }
            else {
                $("#InPatientdRoomNo").val("");
                $("#Name").val("");
            }
        });
        LoadCanteen();
        $('.error1').hide();
        $('#empdiv').hide();
        $('.outerdiv').hide();
        $('.card').hide();
        $('.UPI').hide();
        $('.InPatientd').hide();
        $(".Credit").hide();
        ItemSearch();
        EmployeeSearch();
        if (captureSaleId != 0) {
            $(".CancleCanteen").hide();

            $("#selctedCanteenId").val(captureSaleId);
            onView();
        }
        $("#Discount").change(function () {
            var discount = $("#Discount").val();
            var subTotal = parseInt($('#subTotal').text());
            var dis = parseInt($("#Discount").val());
            if (subTotal >= dis) {
                if (discount == "") {
                    $('#TotalAmount').val($('#subTotal').text());
                }
                else {
                    $('#TotalAmount').val(subTotal - discount + ".00");
                }
            }
            else {
                alert("Discount amount should not be Greater than Sub total amount");
                $("#Discount").val("");
            }
        });
        $('input[type=radio][name=item]').change(function () {
            $('#TransactionNumber').val("");
            $('#NameOfTheCard').val("");
            $('#CardNumber').val("");
            $("#NameOfTheCard1").val("");
            if (this.value == 'Card') {
                $('.card').show();
                $('.UPI').hide();
            }
            else if (this.value == 'UPI') {
                $('.card').hide();
                $('.UPI').show();
            }
            else if (this.value == 'Cash') {
                $('.card').hide();
                $('.UPI').hide();
            } else {
                $('.card').hide();
                $('.UPI').hide();
            }
        });
        $("#ddltypeofselect").change(function () {
            if (ModeofPayment == "Credit") {
                $('input[name=item]').attr('checked', false);
            }

            $("#EmpId").select2("trigger", "select", { data: { id: "", text: "" } });
            $("#InPatientd").select2("trigger", "select", { data: { id: "", text: "" } });
            $('#InPatientdRoomNo').val("");
            $('#Name').val("");
            $('#mobile').val("");
            $('#PurchaserName').val("");
            $('#empdiv').hide();
            $('.outerdiv').hide();
            $('.InPatientd').hide();
            $(".Credit").hide();
            var val = $(this).val();
            if (val == "Emp") {
                $('#empdiv').show();
            }
            else if (val == 'Outer') {
                $('.outerdiv').show();
            } else if (val == 'InPatient') {
                $('.InPatientd').show();
                $(".Credit").show();
            }
        });

    });
    function subItemvalue(id) {

        if ($("#ItemQty" + (id)).val() > 1) {
            var val = parseInt($("#ItemQty" + (id)).val()) - 1;
            $("#ItemQty" + (id)).val(val);
            onchangeQty(val,id);
        }
    }
    function AddItemvalue(id) {
        debugger;
        if ($("#ItemQty" + (id)).val() < 30) {
            var val = parseInt($("#ItemQty" + (id)).val()) + 1;
            $("#ItemQty" + (id)).val(val);
            onchangeQty(val, id);
        }

    }
    function AddItem() {
        var id = $("#ItemId").val();
        $.get("/CMS/GetCMS_Item?id=" + id, function (res) {

            var table = document.getElementById("ItemsTable");
            var table_len = (table.rows.length+1);
            table.insertRow(table.rows.length).outerHTML = "<tr class='datarow' id='row" + tableRowCount + "'><td class='Id" + tableRowCount + "' style='display:none;'>0</td><td style='display:none;'><label id='ItemId" + tableRowCount + "'></label></td><td><label id='ItemName" + tableRowCount + "'></label></td><td><label id='ItemPrice" + tableRowCount + "'></label></td><td><div><span class='glyphicon glyphicon-minus' style='color:red;cursor:pointer;  border: 1px solid green;' onclick='subItemvalue(" + tableRowCount + ")'></span>&nbsp;<input type='number' disabled id='ItemQty" + tableRowCount + "' value='1' min='1' max='30'  />&nbsp;<span class='glyphicon glyphicon-plus' style='color:red;cursor:pointer; border: 1px solid green;'' onclick='AddItemvalue(" + tableRowCount + ")'></span></div> </td><td style='display:none;'> <label class='Packagingcost' id='Packaging" + tableRowCount + "'></label> </td><td><label id='subtotal" + tableRowCount + "'></label></td><td> <span class='glyphicon glyphicon-trash' style='color:red;cursor:pointer;' onclick='delete_row(" + tableRowCount + ")'></span></td></tr>";
            $("#ItemId" + (tableRowCount)).text(id);
            if ($("#ddltypeofselect").val() == "Emp") {
                $("#ItemPrice" + (tableRowCount)).text(res.TotalCostStaff  + ".00");
                //$("#subtotal" + (tableRowCount)).text((res.TotalCostStaff + res.PackagingCost) + ".00");
                $("#subtotal" + (tableRowCount)).text((res.TotalCostStaff) + ".00");
            } else {
                $("#ItemPrice" + (tableRowCount)).text(res.TotalCost  + ".00");
                //$("#subtotal" + (tableRowCount)).text((res.TotalCost + res.PackagingCost)+ ".00");
                $("#subtotal" + (tableRowCount)).text((res.TotalCost) + ".00");
            }

            $("#Packaging" + (tableRowCount)).text(res.PackagingCost);
            $("#ItemName" + (tableRowCount)).text(res.ItemName);

            if (table_len != 1) {
                var subTotal = parseInt($('#subTotal').text());
                if ($("#ddltypeofselect").val() == "Emp") {
                    //subTotal = subTotal + parseInt(res.TotalCostStaff + res.PackagingCost);
                    subTotal = subTotal + parseInt(res.TotalCostStaff);
                } else {
                    //subTotal = subTotal + parseInt(res.TotalCost + res.PackagingCost);
                    subTotal = subTotal + parseInt(res.TotalCost);
                }
                $('#subTotal').text(subTotal + ".00");
                $('#TotalAmount').val(subTotal + ".00");
            }
            else {
                if ($("#ddltypeofselect").val() == "Emp") {
                    //$('#subTotal').text((res.TotalCostStaff + res.PackagingCost) + ".00");
                    //$('#TotalAmount').val((res.TotalCostStaff + res.PackagingCost) + ".00");
                    $('#subTotal').text((res.TotalCostStaff) + ".00");
                    $('#TotalAmount').val((res.TotalCostStaff) + ".00");
                } else {
                    //$('#subTotal').text((res.TotalCost + res.PackagingCost) + ".00");
                    //$('#TotalAmount').val((res.TotalCost + res.PackagingCost) + ".00");
                    $('#subTotal').text((res.TotalCost) + ".00");
                    $('#TotalAmount').val((res.TotalCost) + ".00");
                }
            }
            //Addqty(tableRowCount);
            tableRowCount++;
        });
    }
    function delete_row(no) {
        debugger;
        var subTotal = parseInt($('#subTotal').text());
        subTotal = subTotal - parseInt($("#subtotal" + (no)).text());
        $('#subTotal').text(subTotal + ".00");
        $('#TotalAmount').val(subTotal + ".00");
        $('#row' + no  ).remove();
        //document.getElementById("row" + no + "").outerHTML = "";
        var table = document.getElementById("ItemsTable");
        var table_len = (table.rows.length - 1);
        if (table_len == 0) {
            $('#ItemTable').hide();
        }
        tableRowCount = tableRowCount - 1;
    }
    function onchangeQty(val,id) {
        var amount = parseInt($("#ItemPrice" + (id)).text()) * parseInt(val);
        //var packing = parseInt($("#Packaging" + (id)).text()) * parseInt(val);
        //$("#subtotal" + (id)).text((amount + packing) + ".00");
        $("#subtotal" + (id)).text((amount) + ".00");
        var table = document.getElementById("ItemsTable");
       var table_len = (table.rows.length);
        var subTotal = 0;

        for (var i = 1; i < tableRowCount; i++) {
            var s= $("#subtotal" + (i)).text();
            if ( s != "")
            subTotal = subTotal + parseInt($("#subtotal" + (i)).text());
        }
        $('#subTotal').text(subTotal + ".00");
        var Discount = $('#Discount').val();
        if (Discount != "") {
            $('#TotalAmount').val((subTotal - parseInt($('#Discount').val())));
        }
        else {
            $('#TotalAmount').val(subTotal + ".00");
        }
    }
    function Addqty(id) {
        var options = [];
                for (var i = 1; i < 31; i++) {
                    options.push('<option value="' + i + '">' + i + '</option>');
                }
        $("#ItemQty" + (id)).html(options.join(''));
    }
    function ItemSearch() {
        $("#ItemId").select2({
            ajax: {
                url: "/CMS/GetItemSearch",
                type: "get",
                dataType: 'json',
                data: function (params) {
                    return {
                        searchTerm: params.term // search term
                    };
                },
                processResults: function (response) {
                    return {
                        results: response
                    };
                },
                cache: true
            },
            width: 'element',
            minimumInputLength: 2
        });
    }
    function EmployeeSearch() {
        $.ajaxSetup({ async: false });
        $("#EmpId").select2({
            ajax: {
                url: "/Home/GetEmployeeSearch",
                type: "get",
                dataType: 'json',
                //delay: 250,
                data: function (params) {
                    return {
                        searchTerm: params.term // search term
                    };
                },
                processResults: function (response) {
                    return {
                        results: response
                    };
                },
                cache: true
            },
            minimumInputLength: 3
        });
        $.ajaxSetup({ async: false });
        $("#InPatientd").select2({
            ajax: {
                url: "/CMS/GetInPatientdSearch",
                type: "get",
                dataType: 'json',
                //delay: 250,
                data: function (params) {
                    return {
                        searchTerm: params.term // search term
                    };
                },
                processResults: function (response) {
                    return {
                        results: response
                    };
                },
                cache: true
            },
            minimumInputLength: 3
        });
    }
    function LoadCanteen() {
        $.ajax({
            type: "POST",
            url: "/CMS/GetStore",
            //data: "id=",
            success: function (data) {
                var options = [];
                options.push('<option value="">- Select -</option>');
                for (var i = 0; i < data.length; i++) {
                    if (data[i].TypeOf == "Canteen") {
                        options.push('<option value="' + data[i].StoreId + '">' + data[i].StoreName + '</option>');
                    }
                }
                $("#CanteenId").html(options.join(''));
                },
            error: function () {
                alert("error");
            }
        });
    }
    function validatedetails() {
        setTimeout(function () { $('.error1').fadeOut('fast'); }, 5000);
        var valid = true;
        $(".error").hide();
        $(".css-error").removeClass('css-error');
        //if (!inputvalidation('type', 'Select Type')) valid = false;
        if ($("#ddltypeofselect").val() == "") {
            $("#ddltypeofselect").after('<span class="error" style="color:Red;">Select Type</span>');
            $("#ddltypeofselect").addClass('css-error');
            valid = false;
        }
        else if ($("#ddltypeofselect").val() == "Emp") {
            if ($('#EmpId').val() == "") {
                $('.error1').show();
                $('.error1').text("Plese Employee");

                valid = false;
            }
        }
        else if ($("#ddltypeofselect").val() == "Outer") {

        }
        else if ($("#ddltypeofselect").val() == "InPatient") {
            if ($("#Name").val() == "") {
                $("#Name").after('<span class="error" style="color:Red;">Enter Name</span>');
                $("#Name").addClass('css-error');
                valid = false;
            }
            if ($("#InPatientd").val() == "") {
                $("#InPatientd").after('<span class="error" style="color:Red;">Enter Patient Id</span>');
                $("#InPatientd").addClass('css-error');
                valid = false;
            }
            if ($("#InPatientdRoomNo").val() == "") {
                $("#InPatientdRoomNo").after('<span class="error" style="color:Red;">Enter Room No</span>');
                $("#InPatientdRoomNo").addClass('css-error');
                valid = false;
            }
        }
        if (valid==true) {
            if (tableRowCount <= 1) {
                $('.error1').show();
                $('.error1').text("Plese Select Any Item");

                //alert("Plese Select Any Item");
                valid = false;
            }
        }
        if (valid == true) {
            debugger;
            if ($('input[name="item"]:checked').length == 0) {
                $('.error1').show();
                $('.error1').text("Plese Select Payment type");
                valid = false;
            }
        }
        return valid;
    }
    function DeleteSales() {
        if (confirm("Are you sure you want to delete?")) {
            $.get("/CMS/DeleteSales?id=" + captureSaleId, function (data) {
                alert(data);
                location.reload();
            });
        }
    }
    function SaveDetails() {
        var valid = validatedetails();
        debugger;
        if (valid) {
            var Nameofthecard = $("#NameOfTheCard").val();
            if ($("input[name='item']:checked").val() == "UPI")
                Nameofthecard = $("#NameOfTheCard1").val();
            var task = {
                TransactionId: $("#selctedCanteenId").val(),
                TransactionCustomerType: $("#ddltypeofselect").val(),
                EmpId:parseInt( $("#EmpId").val()),
                TotalPrice: parseFloat( $("#subTotal").text()).toFixed(2),
                ModeOfPayment: $("input[name='item']:checked").val(),
                CardNumber: $("#CardNumber").val(),
                NameOfTheCard: Nameofthecard,
                DiscountValue: parseFloat($("#Discount").val()).toFixed(2),
                FinalPrice: parseFloat($("#TotalAmount").val()).toFixed(2),
                TotalPaidAmount: parseFloat($("#TotalAmount").val()).toFixed(2),
                EmpName: $("#Name").val(),
                EmpMobile: $("#mobile").val(),
                CanteenId: eId,
                SaleType: $("#ddltypeofselect").val(),
                PatientId: $("#InPatientd").val(),
                InPatientdRoomNo: $("#InPatientdRoomNo").val(),
                IsActive: true
            };
            //if (task.SaleType == "InPatient") {
            //    task.EmpName = $("#PurchaserName").val();
            //}
            if (task.ModeOfPayment == "UPI") {

                task.CardNumber = $("#TransactionNumber").val();
            }
            $.ajax({
                type: "POST",
                url: "/CMS/UpdateCMSTransaction",
                data: task,
                success: function (data) {
                    SaveItem(data);
                }
            });
        }
    }
    function SaveItem(id) {
        var ItemList = [];

        for (var i = 1; i < tableRowCount; i++) {
            var row = $('#row' + i).html();
            if (typeof (row) === "undefined") {

            }
            else {
                ItemList.push({
                    //Id: parseInt($("#id" + i).text()),
                    TransactionId: parseInt(id),
                    ItemId: parseInt($("#ItemId" + i).text()),
                    Qty: parseInt($("#ItemQty" + i).val()),
                    FinalAmount: parseFloat($("#subtotal" + i).text()).toFixed(2)
                });
            }

        }

        $.post("/CMS/UpdateTransactionItemList", { model: ItemList }, function (res) {
            alert("Successfully Saved");
            location.reload();
        });
    }
    //view DetailsByID
    function onView() {
        var id = $("#selctedCanteenId").val();
        $("#NoCapture").hide();
        $(".capture").hide();
        $.ajaxSetup({ async: false });
        $('#ItemsTable tr:not(:first)').remove();
        if (id != "") {
            $.get("/CMS/GetCaptureSales?id=" + id, function (res) {
                debugger;
                if (res.transactions != null) {
                    $(".capture").show();
                    $("#ddltypeofselect").val(res.transactions.TransactionCustomerType);
                    if (res.Emp != "") {
                        $("#EmpId").select2("trigger", "select", { data: { id: res.transactions.EmpId, text: res.Emp } });
                        $('#empdiv').show();
                    }
                    else if (res.patient != "") {
                        $("#InPatientd").select2("trigger", "select", { data: { id: res.transactions.PatientId, text: res.patient } });
                        $('.InPatientd').show();
                        $(".Credit").show();
                    }
                    else {
                        $('.outerdiv').show();
                    }

                    $("#InPatientdRoomNo").val(res.transactions.InPatientdRoomNo);
                    $("#Name").val(res.transactions.EmpName);
                    $("#mobile").val(res.transactions.EmpMobile);
                    $("#TotalAmount").val(res.transactions.TotalPaidAmount);
                    $("#TotalAmount").val(res.transactions.FinalPrice);
                    $("#Discount").val(res.transactions.DiscountValue);

                    $("#CardNumber").val(res.transactions.CardNumber);
                    $("input[name=item][value=" + res.transactions.ModeOfPayment + "]").prop('checked', true).trigger('change');

                    if (res.transactions.ModeOfPayment == "Card") {
                        $("#NameOfTheCard").val(res.transactions.NameOfTheCard);
                        $("#CardNumber").val(res.transactions.CardNumber);
                        $('.card').show();
                    }
                    else if (res.transactions.ModeOfPayment == "UPI") {
                        $("#NameOfTheCard1").val(res.transactions.NameOfTheCard);
                        $("#TransactionNumber").val(res.transactions.CardNumber);
                        $('.UPI').show();
                    }
                    else if (res.transactions.ModeOfPayment == "Credit") {
                        ModeofPayment = res.transactions.ModeOfPayment;
                    }

                    $("#subTotal").text(res.transactions.TotalPrice);
                    AddItems(res.items);
                } else {
                    $("#NoCapture").show();
                }

            });
        } else {
            alert("please Enter CaptureSalesID");
        }

    }
