    function LoadFullcaledar(id, geturl) {
        $('#' + id).fullCalendar({
            header: {
                left: 'prev,next today',
                center: 'title',
                right: 'listWeek'
            },
            //defaultDate: '2019-08-10',
            navLinks: true,
            defaultView: 'listWeek',
            editable: true,
            eventLimit: true, // allow "more" link when too many events
            events: {
                url: geturl,
                data: function () { // a function that returns an object
                    return {
                        LocationId: 1,
                    };

                }
            },
            eventClick: function (calEvent, jsEvent, view) {
                evetclikdeid = calEvent.id;
                fullinfo(calEvent.id);
            },
            eventDrop: function (event, dayDelta, minuteDelta, allDay, revertFunc) {
                if (confirm("Confirm move?")) {
                    UpdateEvent(event.id, event.start._i, event.start._d);
                }
                else {
                    revertFunc();
                }
            },

            eventResize: function (event, dayDelta, minuteDelta, revertFunc) {

                if (confirm("Confirm change appointment length?")) {
                    UpdateEvent(event.id, event.start, event.end);
                }
                else {
                    revertFunc();
                }
            },

            dayClick: function (date, allDay, jsEvent, view) {
                var start = moment(date).format('YYYY-MM-DDTHH:mm:ssZ');
                var date = new Date(start);
                $('#eventTitle').val("");
                $('#eventDate').val(date.toLocaleDateString());

                ShowEventPopup(date);
            }
        });
    }

    function LoadData(tableid, renewal, Expired) {
        $('#' + tableid).dataTable({
            "bServerSide": true,
            "sAjaxSource": "/Task/AjaxGetAdminTracker",
            "bProcessing": true,
            "bDestroy": true,
            "aLengthMenu": [[10, 20, 30, 50, 100, 150, 200, -1], [10, 20, 30, 50, 100, 150, 200, "All"]],
            "aoColumns": [
                { "sName": "TrackerId" },
                { "sName": "LocationId" },
                //{ "sName": "CategoryId" },
                { "sName": "Title" },
                { "sName": "FormType" },
                //{ "sName": "Description" },
                { "sName": "StartDate" },
                { "sName": "EndDate" },
                { "sName": "NextRenewalDate" },
                { "sName": "Status" },
                {
                    "sName": "TrackerId",
                    "bSortable": false,
                    "render": function (data) {
                        return '<a href="/Task/ManageTask/' + data + '" id="' + data + '"><span class="glyphicon glyphicon-th-list"></span>View More</a>';

                    }
                }
            ],
            "fnServerParams": function (aoData) {
                aoData.push(
                    { "name": "renewal", "value": renewal },
                    { "name": "Expired", "value": Expired }
                );
            },
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {

                switch (aData[9]) {
                    case 'Valid':
                        //$(nRow).css('class', 'myClassred');
                        $('td:eq(9)', nRow).css('color', '#681622').css('font-weight', 'bold');
                        break;
                    case 'New':
                        //$(nRow).css('background-color', 'green')
                        $('td:eq(9)', nRow).css('color', '#0e8732').css('font-weight', 'bold');
                        break;
                    case 'Not using':
                        $('td:eq(9)', nRow).css('color', '#fcd44c').css('font-weight', 'bold');
                        break;
                    case 'Others':
                        $('td:eq(9)', nRow).css('color', '#915f6d').css('font-weight', 'bold');
                        break;
                    case 'Expired':
                        $('td:eq(9)', nRow).css('color', '#f54242').css('font-weight', 'bold');
                        break;
                    case ' Renew In Progress':
                        $('td:eq(9)', nRow).css('color', '#a71f23').css('font-weight', 'bold');
                        break;
                    default:
                        $('td:eq(9)', nRow).css('color', 'Green').css('font-weight', 'bold');
                        break;
                }
            }

        });
        $("#" + tableid + "_length").addClass("col-md-6");
        $("#" + tableid + "_length").addClass("col-md-6");
    }
