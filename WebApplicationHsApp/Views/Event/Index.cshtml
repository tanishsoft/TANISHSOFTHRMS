    function onchangedPaymentType() {
        var value = $("#dPaymentType").val();
        if (value == "Cash") {
            $(".ptypetrn").hide();
            $(".Creditcard").hide();
        } else if (value == "Paytm") {
            $(".ptypetrn").show();
            $(".Creditcard").hide();
        }
        else if (value == "Debit_Credit_Card") {
            $(".ptypetrn").hide();
            $(".Creditcard").show();
        }
    }
    $(document).ready(function () {
