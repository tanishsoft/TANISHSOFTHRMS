<script>
    var eventid, validate, MealTypes, ConferenceWorkshop;
    function RegisterDelegate(id) {
        eventid = id;
        $.get("/Event/GetconandworkshopFullDetails", { eventId: id }, function (List) {

            var html = "";
            $("#tableCW tbody").empty();
            for (var i = 0; i < List.length; i++) {

                ConferenceWorkshop = List;
                var Edate = new Date(parseInt(List[i].EventDate.substr(6)));
                var year = Edate.getFullYear();
                var month = Edate.getMonth() + 1;
                if (month < 10) {
                    month = "0" + month;
                }
                var day = Edate.getDate();
                if (day < 10) {
                    day = "0" + day;
                }
                html += "<tr><td><span  id='ECWdate" + i + "'>" + day + '/' + month + '/' + year + "</span></td>"
                if (List[i].IsConference == true) {
                    html += "<td><input type='text'  style='width:70px;float:left;' disabled id='ConferenceAmount" + i + "' class='form-control' value='" + List[i].ConferenceAmount + "' /><label><input type='checkbox' class='cbcheckboxevent' id='eConference" + i + "' autocomplete='off' onchange=CheckboxChange('ConferenceAmount" + i + "','eConference" + i + "'); ><span>Conference</span></label ></td>";
                }
                else {
                    html += "<td>No Conference</td>";
                }
                if (List[i].IsWorkshop == true) {
                    html += "<td><input type='text' style='width:70px;float:left;' id='WorkshopAmount" + i + "' class='form-control' disabled value='" + List[i].WorkshopAmount + "'/> <label > <input type='checkbox' class='cbcheckboxevent' id='eWorkshop" + i + "' autocomplete='off' onchange=CheckboxChange('WorkshopAmount" + i + "','eWorkshop" + i + "'); > <span>Workshop</span></label ></td >";
                }
                else {
                    html += "<td> No WorkShop</td>";
                }
                if (List[i].IsWorkshop == true && List[i].IsConference == true) {
                    html += "<td><input type='text' class='form-control' id='Amount" + i + "' disabled value='" + List[0].Amount + "'/></td></tr>";
                }
                else {
                    html += "<td>Need to select only one </td></tr>";
                }
                // html += "<tr><td><span  id='ECWdate" + i + "'>" + day + '-' + month + '-' + year + "</span></td><td><input type='text' disabled id='ConferenceAmount" + i + "' class='form-control' value='" + List[0].ConferenceAmount + "'/><label class='btn btn-primary'><input type='checkbox' id = 'eConference" + i + "' autocomplete ='off'  onchange=CheckboxChange('ConferenceAmount" + i + "','eConference" + i + "'); ><span>Conference</span></label ></td><td><input type='text' id='WorkshopAmount" + i + "' class='form-control' disabled value='" + List[0].WorkshopAmount + "'/> <label class='btn btn-success'><input type='checkbox' id = 'eWorkshop" + i + "' autocomplete ='off' onchange=CheckboxChange('WorkshopAmount" + i + "','eWorkshop" + i + "'); ><span>Workshop</span></label ></td><td><input type='text' class='form-control' id='Amount" + i + "' disabled value='" + List[0].Amount +"'/></td></tr>";
            }
            $("#tableCW").append(html);
            $("#myModalDelegate").modal('show');
            $(".Registrationtype").css("display", "block");
            $("#dStatus").val("DELEGATE");
        });
    }
    function CheckboxChange(confwrkid, checkbox) {
        var totalamount = $("#dTotalAmount").val();
        var value = $("#" + confwrkid).val();
        var checkboxval = $("#" + checkbox).prop('checked');
        if (checkboxval) {
            totalamount = parseFloat(value) + parseFloat(totalamount);
            $("#dTotalAmount").val(totalamount);
            $("#dFinalBlanceTopay").val(totalamount);
        } else {
            totalamount = parseFloat(totalamount) - parseFloat(value);
            $("#dTotalAmount").val(totalamount);
            $("#dFinalBlanceTopay").val(totalamount);
        }
    }
    function OnchangeDiscount() {
        var discount = parseFloat($("#dDiscount").val());
        var totalamount = parseFloat($("#dTotalAmount").val());
        $("#dFinalBlanceTopay").val(totalamount - discount);
    }
    function funcheckboxchange() {
        if ($("#cbFullEvent").is(":checked")) {
            var amountforevent = $("#amountforevent_" + eventid).val();
            if (amountforevent == null) {
                amountforevent = 0;
            }
            $("#dTotalAmount").val(parseFloat(amountforevent));
            $(".cbcheckboxevent").prop("disabled", true);
        } else {
            $("#dTotalAmount").val(0);
            $(".cbcheckboxevent").removeAttr("disabled");
        }
        OnchangeDiscount();
        $(".cbcheckboxevent").prop('checked', false);
    }
    function MealTypes() {
        $.get("/Event/GetMealTypeFullDetails", { eventId: eventid }, function (mealtype) {

            MealTypes = mealtype;
            var EventStartDate = $("#eEventStartDate_" + id).html();
            var EventEndDate = $("#eEventEndDate_" + id).html();

            var dataPartsSt = EventStartDate.split("/");
            // month is 0-based, that's why we need dataParts[1] - 1
            var dateEventStart = new Date(dataPartsSt[2], dataPartsSt[1] - 1, dataPartsSt[0]);

            var dataPartsEn = EventEndDate.split("/");
            // month is 0-based, that's why we need dataParts[1] - 1
            var dateEventEnd = new Date(dataPartsEn[2], dataPartsEn[1] - 1, dataPartsEn[0]);
            var htmldates = '<option value="">Select</option>';
            var htmlformealtype = "";
            while (dateEventStart <= dateEventEnd) {
                var dd = dateEventStart.getDate();
                var mm = dateEventStart.getMonth() + 1;
                var y = dateEventStart.getFullYear();
                if (mm < 10) {
                    mm = "0" + mm;
                }
                var FormattedDate = dd + '/' + mm + '/' + y;
                htmldates += '<option value="' + FormattedDate + '">' + FormattedDate + '</option>';
                for (var i = 0; i < mealtype.length; i++) {
                    htmlformealtype += "<tr>";
                    htmlformealtype += "<td>" + FormattedDate + "<input type='hidden' id='evntdate_" + mealtype[i].MealTypeId + "' value='" + FormattedDate + "' /></td>";
                    htmlformealtype += "<td>" + mealtype[i].Name + "</td>";
                    htmlformealtype += "<td><input type='number' id='qty_" + mealtype[i].MealTypeId + "' value='1' /></td>";
                    htmlformealtype += "<td><input type='text' id='comments_" + mealtype[i].MealTypeId + "' class='form-control' /></td>";
                    htmlformealtype += "<td><input type='checkbox' id='cbyes_" + mealtype[i].MealTypeId + "' /></td>";
                    htmlformealtype += "</tr>";
                }
                dateEventStart.setDate(dateEventStart.getDate() + 1);
            }
            $("#tblmealtype").html(htmlformealtype);
            $("#BookingFromDate").html(htmldates);
            $("#BookingToDate").html(htmldates);
            $("#myModalDelegate").modal('show');
        });
    }
