<div class="row">
    @foreach (var ev in Model)
    {
        string eventstartdate = ev.EventStartDate != null ? ev.EventStartDate.ToString("dd/MM/yyyy") : "";
        string eventenddate = ev.EventEndDate != null ? ev.EventEndDate.ToString("dd/MM/yyyy") : "";
        <div class="col-md-4">
            <div class="panel panel-default" style="text-align:center;">
                <div class="panel-heading">
                    <h3 class="panel-title" style="color:#c4122f;">@ev.Name</h3>
                </div>
                <div class="panel-body" style="text-align:center;height:320px;">
                    <input type="hidden" id="amountforevent_@ev.EventId" value="@ev.EventTotalAmount" />
                    <p>
                        <span class="glyphicon glyphicon-shopping-cart" style="color:purple;"></span>&nbsp;@ev.NumberOfSeats &nbsp;&nbsp;
                        <span class="glyphicon glyphicon-map-marker" style="color:teal;"></span>&nbsp;@ev.EventLocation &nbsp;&nbsp;
                        <span class="glyphicon glyphicon-calendar" style="color:green;"></span> <span id="eEventStartDate_@ev.EventId">@eventstartdate</span> to <span id="eEventEndDate_@ev.EventId">@eventenddate</span>
                    </p>
                    <img style="height: 200px; width: 100%; display: block;" onerror="this.src='/images/Fernandez.png'" src="~/Uploads/@ev.Document1" alt="image" />
                    <div class="card-body">
                        @if (ev.Description!=null && ev.Description.Length > 200)
                        {
                            <p class="card-text">@ev.Description.Substring(0, 200)</p>
                        }
                        else
                        {
                            <p class="card-text">@ev.Description</p>
                        }
                    </div>
                    <div>
                        <a href="/Event/EventMeals/@ev.EventId" class="btn btn-success right">Event Meal</a>&nbsp;
                        <a href="/Event/EventWelcomeKit/@ev.EventId" class="btn btn-warning right">Welcome Kit</a>&nbsp;
                        <button type="button" class="btn btn-success left" id="@ev.EventId" onclick="RegisterDelegate(this.id);">Register Delegate</button>
                        <div class="btn-group right">
                            <div class="btn btn-default dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-th-list"></span></div>
                            <ul class="dropdown-menu">
                                <li><a href="/Event/ViewDelegates/@ev.EventId">View All Delegates</a></li>
                                <li><a href="/Event/ViewPayments/@ev.EventId">View Payments</a></li>
                                <li><a href="/Event/ViewEvent?eventId=@ev.EventId">More Details</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }

</div>
