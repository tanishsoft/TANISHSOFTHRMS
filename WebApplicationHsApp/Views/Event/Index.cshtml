    function ddldeleagetypechange() {
        var value = $("#dStatus").val();
        $(".Registrationtypeemp").css("display", "none");
        $(".Registrationtypesp").css("display", "none");
        if (value == "DELEGATE") {
            $(".Registrationtype").css("display", "block");

        } else {
            $(".Registrationtype").css("display", "none");
            if (value == "SPONSOR") {
                $(".Registrationtypesp").css("display", "block");
            }
            if (value == "EMPLOYEE") {
                $(".Registrationtypeemp").css("display", "block");
            }
        }
    }
