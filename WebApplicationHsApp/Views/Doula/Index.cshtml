                        <input type="button" id="btnspecific" class="btn btn-primary" value="Save" onclick="SaveSepcificDoula();" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>
                                        Id
                                    </th>
                                    <th>Name</th>
                                    <th>Action</th>
                                </tr>
                            </thead>

                            <tbody id="tblmgdoula"></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>