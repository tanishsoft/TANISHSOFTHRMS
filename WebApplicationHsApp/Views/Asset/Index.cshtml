                    options.push('<option value="' + data[i].AssetId + '">' + data[i].Name + ' ' + data[i].SerialNumber + '</option>');
                }
                $("#ParentAssetName").html(options.join(''));
                $("#ParentAssetName").select2();
                //$("#ddlAsset").html(options.join(''));

                //$("#EditParentAssetName").html(options.join(''));
            });
        }
        else {
