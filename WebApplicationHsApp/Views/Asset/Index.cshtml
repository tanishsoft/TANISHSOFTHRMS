                            html += '<li><a href="/Custom/PrintMobilePhonePolicy/' + id + '" id="' + id + '" target="_blank">Print</a></li>';
                        } else {
                            html += '<li><a href="javascript:void(0);"  onclick="Delete(' + id + ')">Cancel</a></li>' +
