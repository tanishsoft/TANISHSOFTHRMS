                AllocationStatus: $("input[name='AllocationStatus']:checked").val(),
                LocationId: $("#ddlLocation").val(),
                DepartmentId: $("#ddlDepartment").val()
            };
            $.ajax({
                type: "POST",
                url: "/Asset/SaveNewUserAsset",
                data: task,
                success: function (data) {
                    alert("Success");
                    SendEmail(data);
                    window.location.reload();
                 
