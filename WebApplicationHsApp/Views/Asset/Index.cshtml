                aoData.push(
                    { "name": "locationid", "value": $("#ddllocation1").val() },
                    { "name": "departmentid", "value": $("#ddldepartment12").val() },
                    { "name": "typeofitem", "value": $("#ddlRequesttype").val() },
                    { "name": "status", "value": $("#ddlStatus").val() },
                    { "name": "Emp", "value": $("#ddlEmployee123").val() }
