    function Loadassetsbyuser(id) {
        $.get("/Asset/GetAssetListbyUser?userId=" + id, function (data) {
            var html = "<table class='table table-bordered'><tr><td>Name</td><td>Asset Serial Number</td><td>Allocation Status</td><td>DateOfAssign</td></tr>";
            for (var i = 0; i < data.length; i++) {
                html += "<tr><td>" + data[i].AssetName + "</td><td>" + data[i].AssetSerialNumber + "</td><td>" + data[i].AllocationStatus + "</td><td>" + data[i].DateOfAssign +"</td></tr>";
            }
            html += "</table>";
            $("#tbluserasset").html(html);
        });
