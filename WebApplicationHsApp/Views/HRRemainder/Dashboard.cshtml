        LoadData("tbl15daysToRenewal", "15 Days", "");
        LoadData("tbl30daysToRenewal", "30 Days", "");
        LoadData("tbl60daysToRenewal", "60 Days", "");
        LoadData("tbl90daysToRenewal", "90 Days", "");
    });
    function LoadHrRemainderByType() {
        $.ajax({
            type: "GET",
            url: "/HRRemainder/GetRemaindersCountByType",
            //data: "id=",
            success: function (data) {
                var locdata = new Array();
                for (var i = 0; i < data.length; i++) {
                    locdata.push({
                        name: data[i].Key,
                        y: data[i].Value,
                        drilldown: true
                    });
                }
                Binddatatochart(locdata, "Remainders By Type", "container3");
            },
            error: function () {
                alert("error")
            }
        });
    }
    function LoadHrRemainderByStatus() {
        $.ajax({
            type: "GET",
            url: "/HRRemainder/GetRemaindersCountByStatus",
            //data: "id=",
            success: function (data) {
                var locdata = new Array();
                for (var i = 0; i < data.length; i++) {
                    locdata.push({
                        name: data[i].Key,
                        y: data[i].Value,
                        drilldown: true
                    });
                }
                Binddatatochart(locdata, "Remainders By Status", "container4");
            },
            error: function () {
                alert("error")
            }
        });
    }
    function Binddatatochart(locations, title, id) {

        $('#' + id).highcharts({
            chart: {
                type: 'column',
                events: {
                    drilldown: function (e) {
                        if (!e.seriesOptions) {

                            var chart = this;
                            chart.showLoading('Simulating Ajax ...');
                            // var series = GetseriesDepttomorrow(e.point.name, chart, e.point);
                            // Show the loading label

                        }
                    }
                }
            },
            title: {
                text: title
            },
            xAxis: {
                type: 'category'
            },

            legend: {
                enabled: false
            },

            plotOptions: {
                series: {
                    borderWidth: 0,
                    dataLabels: {
                        enabled: true
                    }
                }
            },

            series: [{
                name: title,
                colorByPoint: true,
                data: locations
            }],

            drilldown: {
                series: []
            }
        });
    }

    function LoadData(tableid, renewal, Expired) {
        $('#' + tableid).dataTable({
            "bServerSide": true,
            "sAjaxSource": "/HRRemainder/AjaxGetHRRemainders",
            "bProcessing": true,
            "bDestroy": true,
            "aLengthMenu": [[10, 20, 30, 50, 100, 150, 200, -1], [10, 20, 30, 50, 100, 150, 200, "All"]],
            "aoColumns": [
                { "sName": "TrackerId" },
                { "sName": "LocationId" },
                //{ "sName": "CategoryId" },
                { "sName": "Title" },
                { "sName": "FormType" },
                //{ "sName": "Description" },
                { "sName": "StartDate" },
                { "sName": "EndDate" },
                { "sName": "NextRenewalDate" },
                { "sName": "Status" },
                {
                    "sName": "TrackerId",
                    "bSortable": false,
                    "render": function (data) {
                        return '<a href="/Task/ManageTask/' + data + '" id="' + data + '"><span class="glyphicon glyphicon-th-list"></span>View More</a>';

                    }
                }
            ],
            "fnServerParams": function (aoData) {
                aoData.push(
                    { "name": "renewal", "value": renewal },
                    { "name": "Expired", "value": Expired }
                );
            },
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {

                switch (aData[9]) {
                    case 'Valid':
                        //$(nRow).css('class', 'myClassred');
                        $('td:eq(9)', nRow).css('color', '#681622').css('font-weight', 'bold');
                        break;
                    case 'New':
                        //$(nRow).css('background-color', 'green')
                        $('td:eq(9)', nRow).css('color', '#0e8732').css('font-weight', 'bold');
                        break;
                    case 'Not using':
                        $('td:eq(9)', nRow).css('color', '#fcd44c').css('font-weight', 'bold');
                        break;
                    case 'Others':
                        $('td:eq(9)', nRow).css('color', '#915f6d').css('font-weight', 'bold');
                        break;
                    case 'Expired':
                        $('td:eq(9)', nRow).css('color', '#f54242').css('font-weight', 'bold');
                        break;
                    case ' Renew In Progress':
                        $('td:eq(9)', nRow).css('color', '#a71f23').css('font-weight', 'bold');
                        break;
                    default:
                        $('td:eq(9)', nRow).css('color', 'Green').css('font-weight', 'bold');
                        break;
                }
            }

        });
        $("#" + tableid + "_length").addClass("col-md-6");
        $("#" + tableid + "_length").addClass("col-md-6");
    }
</script>
