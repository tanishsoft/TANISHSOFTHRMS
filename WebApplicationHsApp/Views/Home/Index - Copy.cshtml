    function GetTodayJoines() {
        $.ajax({
            type: "GET",
            url: "/Home/GetTodayJoines",
            success: function (data) {
                var html = "";
                var month = new Array();
                month[0] = "Jan";
                month[1] = "Feb";
                month[2] = "Mar";
                month[3] = "Apr";
                month[4] = "May";
                month[5] = "Jun";
                month[6] = "Jul";
                month[7] = "Aug";
                month[8] = "Sep";
                month[9] = "Oct";
                month[10] = "Nov";
                month[11] = "Dec";
                if (data != "No Joines") {
                    for (var i = 0; i < data.length; i++) {
                        var date = new Date(parseInt(data[i].DateOfJoining.replace(/[^0-9 +]/g, '')));
                        //var date = new Date(Date.parse(data[i].DateOfJoining));
                        html += '<tr><td><img src="/Images/Employees/' + data[i].EmpId + '.jpg" style="width:53px;height:53px;" alt="img"></td><td><span>' + data[i].FirstName + ' ' + data[i].LastName + '</span> <br>' + data[i].Designation + '</td>';
                        html += '<td><span>' + (date.getUTCDate()) + '</span><br>' + (month[date.getMonth()]) + ' ' + date.getFullYear() + '</td></tr>';
                        //html += "<div style='font-size: 22px;font-style: italic;font-family: cambria;color:teal;padding:5px;'>" + data[i].FirstName + " " + data[i].LastName + " -  " + data[i].DepartmentName + "</div>";
                    }
                    $("#Newjoineess").html(html);
                }
                else {
                    $("#Newjoineess").html("No Joiners");
                }
            },
            error: function () {

            }
        });
    }
