@if (User.IsInRole("Admin") || User.Identity.Name == "867")
{

    <h4 style="color:#33b5e5;border-bottom:solid 1px #33b5e5;">Enter Approvers Intranet Login Id</h4>
    <br />
    <div class="row">

        <div class="col-md-3">
            &nbsp;
        </div>
        <div class="col-md-3" style="text-align:right; color:teal;">
            Select Location :
        </div>
        <div class="col-md-3">
            <select id="ddlLocationApprover" class="form-control" onchange="LoadAllApprovers();">
                <option value="">Select</option>
