
@{
    ViewBag.Title = "StaffMovementRegister";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>
    .maxwidth {
        max-width: 100%;
    }

    #captureSignatureVisitorIn {
        width: 100%;
        height: auto;
        border: 1px solid black;
    }

    #captureSignatureOutSign {
        width: 100%;
        height: auto;
        min-height: 400px;
        border: 1px solid black;
    }
</style>
<script src="~/Scripts/jSignature.min.js"></script>
<script>
    var validate = true;
    var eId = 0;
    var oTable;
    var $sigdiv, $sigdiv2;
    $(document).ready(function () {
        LoadData();
        LoadLocations();
        $sigdiv = $("#captureSignatureOutSign").jSignature({ 'UndoButton': true });
      /*  $sigdiv2 = $("#captureSignatureSecurity").jSignature({ 'UndoButton': true });*/
        $("#txtFromDate").datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: "dd/mm/yy",
            yearRange: "-30:+30",
            Edit: true
        });
        $("#txtToDate").datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: "dd/mm/yy",
            yearRange: "-30:+30",
            Edit: true
        });
        $(".clmrno").show();
        $("#txtMrNo").change(function () {
            var id = $('#txtMrNo').val();
            if (id != "") {
                $.get("/CMS/GetCMS_PatitentNamebyMRno?id=" + id, function (res) {
                    if (res != null) {
                        $("#MobileNo").val(res.MobileNumber);
                        $("#PatientName").val(res.Name);
                    }
                    else {
                        $("#PatientName").val("");
                    }
                });
            }
        });
        $(".Patients").show();
        $(".Visitor").hide();
        $(".Staff").hide();
        $(".dailyvisittype").change(function () {
            $(".Patients").hide();
            $(".Visitor").hide();
            $(".Staff").hide();
            /*if (this.checked) {*/
                $("." + this.value).show();
            //}
        });
    });
    function LoadData() {
        oTable = $('#tblDailyVisting').dataTable({
            "bServerSide": true,
            "sAjaxSource": "/Custom/AjaxGetDailyVisiting",
            "bProcessing": true,
            "bDestroy": true,
            "aLengthMenu": [[10, 20, 30, 50, 100, 150, 200, -1], [10, 20, 30, 50, 100, 150, 200, "All"]],
            "aoColumns": [
                { "sName": "Id" },
                { "sName": "Name" },
                { "sName": "" },
                { "sName": "" },
                { "sName": "" },
                { "sName": "" },
                { "sName": "" },
                { "sName": "" },
                { "sName": "" },
                //{ "sName": "" },
                { "sName": "IsActive" },
                {
                           "sName": "Id",
                           "bSortable": false,
                           "render": function (data, row, r) {

                               return '<div class="btn-group">' +
                                   '<div class="btn btn-default dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-th-list"></span></div>' +
                                   '<ul class="dropdown-menu dropdown-menu-right">' +
                                   '<li><a href="javascript:void(0);"  onclick="ViewDetails(' + data + ')">View</a></li>' +
                                   '<li><a href="javascript:void(0);"  onclick="UpdateDetails(' + data + ')">Update Details</a></li>' +
                                   //'<li><a href="javascript:void(0);"  onclick="onDelete(' + data + ')">Delete</a></li>' +
                                   '</ul></div>';
                           }
                }
            ],
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
                $('td:eq(2)', nRow).html('<a href="javascript:void(0);" id="' + aData[0] + '" onclick="ViewDetails(this.id);">' + aData[2] + '</a>');
            }
            ,
            "fnServerParams": function (aoData) {
                aoData.push(
                    { "name": "locationid", "value": $("#LocationId2").val() },
                    { "name": "fromdate", "value": $("#txtFromDate").val() },
                    { "name": "todate", "value": $("#txtToDate").val() },
                    { "name": "FormType", "value": "Staff" },
                );
            }
        });
        $("#tblDailyVisting_length").addClass("d-none");
        $("#tblDailyVisting_length").addClass("d-sm-block");
        //$("#tblValetParking_length").addClass("col-xs-6");
    }
    function UpdateDetails(id) {
        eId = id;
        $.get("/Custom/checkIfPatientVisitupdated?id=" + id, function (data) {
            if (data != "Ok") {
                window.location = "/Custom/OutUpdate?id=" + id;
            } else {
                alert("Already updated.");
            }
        });
        //$.get("/Custom/GetDailyVisiting?id=" + id, function (data) {
        //    $("#myModalUpdate").modal('show');
        //});
    }
    function ViewDetails(id) {

        eId = id;
        $.get("/Custom/GetDailyVisiting?id=" + id, function (data) {
            var html = "<table class='table table-bordered'>";
            html += "<tr><td>Location</td><td>" + getlocationame(data.LocationId) + "</td></tr>";
            if (data.VisitingType == "Patients") {

                html += "<tr><td>Patient Type</td><td>" + data.PatientType + "</td></tr>";
                html += "<tr><td>Patient Name</td><td>" + data.PatientName + "</td></tr>";
                html += "<tr><td>Mobile No</td><td>" + data.MobileNo + "</td></tr>";
                html += "<tr><td>Mr No</td><td>" + data.MrNo + "</td></tr>";
                html += "<tr><td>Remarks</td><td>" + data.Remarks + "</td></tr>";
            } else if (data.VisitingType == "Visitor") {

             /*   html += "<tr><td>Patient Type</td><td>" + data.PatientType + "</td></tr>";*/
                html += "<tr><td>Visitor Name</td><td>" + data.PatientName + "</td></tr>";
                html += "<tr><td>Mobile No</td><td>" + data.MobileNo + "</td></tr>";
                html += "<tr><td>Coming From</td><td>" + data.ComingFrom + "</td></tr>";
                html += "<tr><td>Whom To Meet</td><td>" + data.WhomToMeet + "</td></tr>";
                html += "<tr><td>Purpose</td><td>" + data.Purpose + "</td></tr>";
                html += "<tr><td>Visitor ID No </td><td>" + data.TokenNo + "</td></tr>";
                if (data.InTime != null && data.InTime != "") {
                    html += "<tr><td>In Time</td><td>" + data.InTime + "</td></tr>";
                }
                if (data.InSign != null && data.InSign != "") {
                    html += "<tr><td>In Sign</td><td><img style='border-radius:50%;width:100px;'src='/Documents/" + data.InSign + "' /></td></tr>";
                }
                if (data.SecuritySignature != null && data.SecuritySignature != "") {
                    html += "<tr><td>Security Signature</td><td><img style='border-radius:50%;width:100px;'src='/Documents/" + data.SecuritySignature + "' /></td></tr>";
                }
                if (data.OutTime != null && data.OutTime != "") {
                    html += "<tr><td>Out Time</td><td>" + data.OutTime + "</td></tr>";
                }
                if (data.OutSign != null && data.OutSign != "") {
                    html += "<tr><td>Out Sign</td><td><img style='border-radius:50%;width:100px;'src='/Documents/" + data.OutSign + "' /></td></tr>";
                }
            }
            else if (data.VisitingType == "Staff") {
                html += "<tr><td>Department Name</td><td>" + data.DepartmentName + "</td></tr>";
                html += "<tr><td>Employee Name</td><td>" + data.PatientName + "</td></tr>";
                html += "<tr><td>Emp Number</td><td>" + data.MrNo + "</td></tr>";
                html += "<tr><td>Mobile No</td><td>" + data.MobileNo + "</td></tr>";
                html += "<tr><td>Purpose</td><td>" + data.Purpose + "</td></tr>";
                html += "<tr><td>In Time</td><td>" + data.InTime + "</td></tr>";
                if (data.InSign != null && data.InSign != "") {
                    html += "<tr><td>In Sign</td><td><img style='border-radius:50%;width:100px;'src='/Documents/" + data.InSign + "' /></td></tr>";
                }
                if (data.OutTime != null && data.OutTime != "") {
                    html += "<tr><td>Out Time</td><td>" + data.OutTime + "</td></tr>";
                }
                if (data.OutSign != null && data.OutSign != "") {
                    html += "<tr><td>Out Sign</td><td><img style='border-radius:50%;width:100px;'src='/Documents/" + data.OutSign + "' /></td></tr>";
                }
            }
            html += "</table>";
            $("#tableview").html(html);
            $("#myModalView").modal('show');
        });

    }
    var locations;
    function LoadLocations() {
        $.ajax({
            type: "POST",
            url: "/Common/GetLocations",
            //data: "id=",
            success: function (data) {
                locations = data;
                var options = [];
                options.push('<option value="">- All Locations -</option>');
                for (var i = 0; i < data.length; i++) {
                    options.push('<option value="' + data[i].LocationId + '">' + data[i].LocationName + '</option>');
                }
                $("#LocationId").html(options.join(''));
                $("#LocationId2").html(options.join(''));
            },
            error: function () {
                alert("error")
            }
        });
    }
    function getlocationame(id) {
        for (var i = 0; i < locations.length; i++) {
            if (locations[i].LocationId == id) {
                return locations[i].LocationName;
            }
        }
        return '';
    }
    function SavedailyVisiting() {
        validateCategory();
        if (validate) {
            var task = {
                DailyVisitingId: eId,
                LocationId: $("#LocationId").val(),
                DepartmentName:"",
                PatientType: $("#PatientType").val(),
                PatientName: $("#PatientName").val(),
                MobileNo: $("#MobileNo").val(),
                MrNo: $("#txtMrNo").val(),
                Remarks: $("#Remarks").val(),
                IsActive: true
            };
            $.ajax({
                type: "POST",
                url: "/Custom/SaveDailyVisiting",
                data: task,
                success: function (data) {
                    $("#myModalnew").modal('hide');
                    clearAll(); oTable.api().ajax.reload(null, false);

                }
            });
        }
    }
    function Loaddynamicdata() {
        oTable.api().ajax.reload(null, false);
    }
    function validateCategory() {
        validate = true;
        $(".css-error").removeClass('css-error');
        if ($("#PatientType").val() == "") {
            $("#PatientType").addClass('css-error');
            validate = false;
        }
        if ($("#PatientName").val() == "") {
            $("#PatientName").addClass('css-error');
            validate = false;
        }
        //if ($("#txtMrNo").val() == "") {
        //    $("#txtMrNo").addClass('css-error');
        //    validate = false;
        //}
        if ($("#MobileNo").val() == "") {
            $("#MobileNo").addClass('css-error');
            validate = false;
        }

    }
    function onDelete(id) {
        if (confirm('Are you sure you want to delete?')) {
            $.getJSON("@Url.Action("DeleteDailyVisiting", "Custom")", { id: id }, function (data) {
                clearAll();
                oTable.api().ajax.reload(null, false);
            });
        } return false;
    }
    function onEdit(id) {
        clearAll();
        eId = id;
        $.get("/Custom/GetDailyVisiting?id=" + id, function (data) {
            $("#txtupVehicleNo").val(data.VehicleNo);
            $("#txtupouttime").val(data.OutTime);
            $("#txtupoutdriver").val(data.OutDriverName);
            $("#txtupRemarks").val(data.Remarks);
            $("#header").html("Edit");
            $("#myModal").modal('show');
        });

    }
    function clearAll() {
        $(".css-error").removeClass('css-error');
        $("#PatientType").val("");
        $("#txtMrNo").val("");
        $("#PatientName").val("");
        $("#MobileNo").val("");
        $("#Remarks").val("");
        $("#LocationId").val("");
        eId = 0;
    }
    function OnclickSave() {
        validate = true;
        $(".css-error").removeClass('css-error');
        if ($("#txtupVehicleNo").val() == "") {
            $("#txtupVehicleNo").addClass('css-error');
            validate = false;
        }
        if ($("#txtupouttime").val() == "") {
            $("#txtupouttime").addClass('css-error');
            validate = false;
        }
        if ($("#txtupoutdriver").val() == "") {
            $("#txtupoutdriver").addClass('css-error');
            validate = false;
        }
        if (validate) {
            var task = {
                ValetParkingId: eId,

                VehicleNo: $("#txtupVehicleNo").val(),
                OutTime: $("#txtupouttime").val(),
                OutDriverName: $("#txtupoutdriver").val(),
                Remarks: $("#txtupRemarks").val()
            };
            $.ajax({
                type: "POST",
                url: "/Custom/UpdateValetParking",
                data: task,
                success: function (data) {

                    closeandclear(); oTable.api().ajax.reload(null, false);

                }
            });
        }
    }
    function closeandclear() {
        $("#txtupVehicleNo").val("");
        $("#txtupouttime").val("");
        $("#txtupoutdriver").val("");
        $("#txtupRemarks").val("");
        $("#myModal").modal('hide');
        clearAll();
    }
    function ExportExcelData() {
        $("#myModalExportExcel").modal('show');
    }
    function FinalExportExcel() {
        if ($("#txtFromDate").val() != null && $("#txtFromDate").val() != "" && $("#txtToDate").val() != null && $("#txtToDate").val() != "") {
            var url = '/Custom/ExportExcelDailyVisiting?FromDate=' + $("#txtFromDate").val() + '&ToDate=' + $("#txtToDate").val() +'&type=Staff';
            window.location = url;

        } else {
            alert("Please select from date and todae");
        }
    }
    function FinalExportPdf() {
        if ($("#txtFromDate").val() != null && $("#txtFromDate").val() != "" && $("#txtToDate").val() != null && $("#txtToDate").val() != "") {
            var url = '/Custom/ExportPdfDailyVisiting?FromDate=' + $("#txtFromDate").val() + '&ToDate=' + $("#txtToDate").val() + '&type=Staff';
            window.location = url;

        } else {
            alert("Please select from date and todae");
        }
    }
    function isnewchange() {
        var value = $("#ddlIsNew").val();
        if (value == "Yes") {
            $(".clmrno").hide();
        } else {
            $(".clmrno").show();
        }
    }
    function closeallnew() {
        $("#myModalnew").modal('hide');
    }
    function newmodalshow() {
        //$("#myModalnew").modal('show');
        window.location.href = "/Register/NewStaffMovementRegister";
    }
</script>
<div class="panel panel-default">
    <div class="panel-heading" style="height:35px;"><a href="~/Register/Index">Go Back</a> &nbsp;&nbsp;  Manage Staff Movement <span style="color:orangered; cursor:pointer; float:right;" onclick="newmodalshow()">&nbsp;&nbsp;&nbsp;&nbsp;New</span>  </div>
    <div class="panel-body">
        <div class="row" style="padding:5px;">
            <div class="col-md-2 col-xs-6">
                <select id="LocationId2" class="form-control" onchange="Loaddynamicdata()">
                    <option value="">Select</option>
                </select>
            </div>
            <div class="col-md-2 col-xs-6">
                <input type="text" class="form-control" placeholder="From Date" id="txtFromDate" readonly="readonly" autocomplete="off" />
            </div>
            <div class="col-md-2 col-xs-6">

                <input type="text" class="form-control" placeholder="To Date" id="txtToDate" readonly="readonly" autocomplete="off" />
            </div>
            <div class="col-md-1 col-xs-6" style="text-align:left !important;">

                <input type="button" class="btn btn-primary" id="btnsave" value="Go" style="float:left;" onclick="Loaddynamicdata();" />
            </div>
            <div class="col-md-1 col-xs-6" style="text-align:left !important;">

                <input type="button" class="btn btn-default" id="btnsave2" value="Export To Excel" style="float:left;" onclick="FinalExportExcel();" />
            </div>
            <div class="col-md-1 col-xs-6" style="text-align:left !important;">
                <input type="button" class="btn btn-secondary" id="btnsave2" value="Export To Pdf" style="float:left;" onclick="FinalExportPdf();" />
            </div>
        </div>
        <div class="row" style="padding:5px;">

            <table class="table table-striped table-bordered dt-responsive nowrap" style="width:100%;" id="tblDailyVisting">
                <thead>
                    <tr style="background-color:chocolate;color:white;">
                        <th>Id</th>
                        <th>Visit Type</th>
                        <th>Location</th>
                        <th>Mr No</th>
                        <th>Name</th>
                        <th>Mobile No</th>
                        <th>Visit For</th>

                        <th>Date</th>
                        <th>Modified By</th>
                        <th>Modified Date</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>

<div id="myModal" class="modal fade" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" onclick="closeandclear();">×</button>
                <h4 class="modal-title" id="header">Update</h4>
            </div>
            <div class="modal-body">
                <div class="row" style="padding:5px;">
                    <div class="col-md-4">
                        <label for="name">Vehicle No<span style="color:red;">*</span></label>
                    </div>
                    <div class="col-md-8">
                        <input type="text" class="form-control" id="txtupVehicleNo" disabled="disabled" autocomplete="off" />
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4">
                        <label for="name">Out Time<span style="color:red;">*</span></label>
                    </div>
                    <div class="col-md-8">

                        <select class="form-control" id="txtupouttime">
                            <option>Select</option>
                            @for (var i = 0; i < 24; i++)
                            {
                                for (var j = 1; j <= 60; j++)
                                {
                                    if (j % 5 == 0)
                                    {
                                        string value = i.ToString();
                                        if (i < 10)
                                        {
                                            value = "0" + value;
                                        }
                                        if (j < 10)
                                        {
                                            value = value + ":0" + j;
                                        }
                                        else
                                        {
                                            value = value + ":" + j;
                                        }
                                        <option value="@value">@value</option>
                                    }
                                }
                            }
                        </select>
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4">
                        <label for="name">Out Driver<span style="color:red;">*</span></label>
                    </div>
                    <div class="col-md-8">
                        <input type="text" class="form-control" id="txtupoutdriver" autocomplete="off" />
                    </div>
                </div>

                <div class="row" style="padding:5px;">
                    <div class="col-md-4">
                        <label for="name">Remarks</label>
                    </div>
                    <div class="col-md-8">
                        <input type="text" class="form-control" id="txtupRemarks" autocomplete="off" />
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <input type="button" id="btnasigntask" value="Update" onclick="OnclickSave();" class="btn btn-primary">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="clearAll();">Close</button>
            </div>
        </div>
    </div>
</div>



<div id="myModalnew" class="modal fade" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" onclick="closeallnew();">×</button>
                <h4 class="modal-title" id="header22">New Daily Visit</h4>
            </div>
            <div class="modal-body">


                <div class="row" style="padding:5px;">
                    <div class="col-md-4">
                        <label for="name">Location<span style="color:red;">*</span></label>
                    </div>
                    <div class="col-md-8">
                        <select id="LocationId" class="form-control">
                            <option value="">Select</option>
                        </select>
                    </div>
                </div>
                <div class="row" style="padding:5px;">
                    <div class="col-md-4">
                        <label for="name">Visit Type<span style="color:red;">*</span></label>
                    </div>
                    <div class="col-md-8">
                        <select id="ddlvisittype" class="form-control dailyvisittype">
                            <option value="">Select</option>
                            <option value="Patients">Patients Visiting</option>
                            <option value="Visitor">Visitor Movement Register</option>
                            <option value="Staff">Staff Movement Register</option>
                        </select>
                    </div>
                </div>
                <div class="Patients">
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Is New</label>
                        </div>
                        <div class="col-md-8">
                            <select id="ddlIsNew" class="form-control" onchange="isnewchange();">
                                <option value="">Select</option>
                                <option value="Yes">Yes</option>
                                <option value="No" selected>No</option>
                            </select>
                        </div>
                    </div>
                    <div class="row clmrno" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">MR NO<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" class="form-control" id="txtMrNo" autocomplete="off" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Patient Name<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" class="form-control" id="PatientName" autocomplete="off" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Mobile No<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" class="form-control" id="MobileNo" autocomplete="off" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Come for<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <select id="PatientType" class="form-control">
                                <option value="">Select</option>
                                <option value="IP">IP</option>
                                <option value="OP">OP</option>
                                <option value="ER">ER</option>
                                <option value="Other">Other</option>
                            </select>
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Remarks<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" class="form-control" id="Remarks" autocomplete="off" />
                        </div>
                    </div>
                </div>

                <div class="Visitor sigPad">
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Visitor Name<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" id="txtVstrName" class="form-control" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Coming From<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" id="txtVstrComingFrom" class="form-control" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Whom To Meet<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" id="txtVstrWhomToMeet" class="form-control" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Contact Number<span style="color:red;">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" id="txtVstrContactNumber" class="form-control" />
                        </div>
                    </div>
                    <div class="row" style="padding:5px;">
                        <div class="col-md-4">
                            <label for="name">Purpose<span style="color:red;">*</span></label>
